<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane prefHeight="739.0" prefWidth="879.0"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="app.arxivorg.controller.FavoritesController">
   <children>
      <ListView fx:id="listView" layoutX="15.0" layoutY="14.0" onMouseClicked="#changeCurrentIndex" prefHeight="711.0" prefWidth="738.0" />
      <Button fx:id="deleteButton" disable="true" layoutX="756.0" layoutY="54.0" mnemonicParsing="false" onAction="#deleteArticle" prefHeight="26.0" prefWidth="112.0" text="supprimer" textFill="WHITE" AnchorPane.leftAnchor="758.0" AnchorPane.rightAnchor="11.0">
         <graphic>
            <FontIcon iconColor="WHITE" iconLiteral="eli-trash" />
         </graphic></Button>
      <Button fx:id="downloadButton" disable="true" layoutX="756.0" layoutY="105.0" mnemonicParsing="false" onAction="#downloadArticle" prefHeight="26.0" prefWidth="112.0" text="télécharger" textFill="WHITE" AnchorPane.leftAnchor="758.0">
         <graphic>
            <FontIcon iconColor="WHITE" iconLiteral="eli-download" />
         </graphic></Button>
   </children>
</AnchorPane>
